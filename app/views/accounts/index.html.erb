<h1>My Accounts</h1>

<div><b>Display </b> <%= select_tag(:display, options_for_select([['All', 'all']] + @user_account_types), class: "form-control display-select") %></div> <br />  

<div id='result-display'>
  <div id='accounts'><%= @accounts.any? ? render(partial: 'accounts') : "No Accounts Available." %></div>  
</div>
<br/><br/>

<div><%= link_to "Create New Account", new_account_path, data: {toggle: 'modal', target: '.bs-example-modal-lg', remote: true}, class: 'btn btn-primary' %></div>
